{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/_assets/*",
    "/build/*",
    "/static/*",
    "/*.ico",
    "/*.jpg",
    "/*.jpeg",
    "/*.png",
    "/*.gif",
    "/*.svg",
    "/*.css",
    "/*.js"
  ],
  "routes": [
    {
      "src": "/(auth|api)/.*",
      "dest": "/worker"
    }
  ]
}